{
  "name": "phillboards",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "npm run web:dev",
    "web:dev": "webpack-dev-server --mode development --config webpack.config.js --open --port 8090",
    "web:build": "webpack --mode production --config webpack.config.js",
    "web:serve": "npx serve -s dist",
    "web:setup": "node setup-web-dependencies.js",
    "eject": "expo eject",
    "postinstall": "node -e \"try { require('fs').mkdirSync('./public', { recursive: true }); } catch (e) {}\"",
    "expo-go": "node start-tunnel.js",
    "hotspot": "node hotspot-mode.js",
    "lan": "expo start --lan",
    "tunnel": "EXPO_USE_CUSTOM_TOKEN_DOMAIN=true expo start --tunnel",
    "build:android": "node build-android.js",
    "setup:android": "powershell -ExecutionPolicy Bypass -File .\\setup-android-env.ps1",
    "prod:android": "eas build -p android --profile preview",
    "prod:ios": "eas build -p ios --profile preview",
    "prod:web": "npm run web:build && npm run web:serve"
  },
  "dependencies": {
    "@expo/config-plugins": "^10.0.2",
    "@expo/metro-runtime": "~3.1.3",
